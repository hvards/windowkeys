{
  "OverlaySettings": {
    "FontFamily": "Consolas",
    "FontSize": 48,
    "BorderColor": "#FF0000",
    "BorderWidth":  5,
    "Opacity":  0.7
  },
  "ActivationSettings": {
    "HotKey": [72, 91],
    "SelectionKeys": ["A", "R", "S", "T", "N", "E", "I", "O"]
  },
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%LOCALAPPDATA%/hvard/windowkeys.log",
          "fileSizeLimitBytes": 16384
        }
      }
    ]
  }
}
